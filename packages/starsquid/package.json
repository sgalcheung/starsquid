{
	"name": "starsquid",
	"version": "1.1.0",
	"description": "Use your Squidex data in Astro projects",
	"author": {
		"email": "sgalculture@outlook.com",
		"name": "Sgal Cheung",
		"url": "https://github.com/sgalcheung"
	},
	"license": "MIT",
	"keywords": [
		"withastro",
		"astro-loader"
	],
	"homepage": "https://starsquid.vercel.app/",
	"repository": {
		"type": "git",
		"url": "https://github.com/sgalcheung/starsquid.git",
		"directory": "packages/starsquid"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		"./loaders": {
			"types": "./dist/loaders.d.ts",
			"default": "./dist/loaders.js"
		},
		"./schemas": {
			"types": "./dist/data/models/schemas.d.ts",
			"default": "./dist/data/models/schemas.js"
		},
		"./integrations": {
			"types": "./dist/integrations.d.ts",
			"default": "./dist/integrations.js"
		},
		"./api": {
			"types": "./dist/data/core/api.d.ts",
			"default": "./dist/data/core/api.js"
		},
		"./squidex": {
			"types": "./squidex.d.ts",
			"default": "./squidex.mjs"
		}
	},
	"files": [
		"dist",
    "README.md",
    "squidex.d.ts",
    "squidex.mjs"
	],
	"scripts": {
		"generate": "ts-to-zod",
		"build": "run-scripts build \"src/**/*.ts\"",
		"dev": "run-scripts dev \"src/**/*.ts\"",
		"test": "vitest"
	},
	"type": "module",
	"peerDependencies": {
		"astro": ">=5.0.0"
	},
	"dependencies": {
		"@squidex/squidex": "2.0.0-rc2",
		"ts-pattern": "^5.7.0"
	},
	"devDependencies": {
		"esbuild": "^0.25.2",
		"ts-to-zod": "^3.14.1",
		"tsup": "^8.3.5"
	}
}
